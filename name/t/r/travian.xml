<?xml version="1.0" encoding="UTF-8"?>
<vpi xmlns='http://virtual-presence.org/schemas/vpi'>

  <location match='^https?://[^/:]*travian\.([^./:]+)($|/.*$)'>
    <service>jabber:muc4.virtual-presence.org</service>
    <name hash="SHA1">travian.\1</name>
    <destination>http://travian.\1</destination>
    <topology level="language"/>
  </location>

  <delegate>
    <uri>../../default.xml</uri>
  </delegate>

</vpi>
